{
  "agent_role": "ci_workflow_generator",
  "repository": {
    "url": "https://github.com/lakshmi1212/Math",
    "branch": "main"
  },
  "workflow": {
    "file_path": ".github/workflows/ci.yml",
    "name": "CI Pipeline"
  },
  "triggers": {
    "push": [".github/workflows"],
    "pull_request": ["main"]
  },
  "runtime": {
    "language": "python",
    "version": "3.10"
  },
  "dependencies": [
    "pytest"
  ],
  "test_execution": {
    "test_directory": "tests",
    "fail_on_error": true
  },
  "constraints": {
    "modify_source_files": false,
    "modify_test_files": false
  },
  "commit": {
    "enabled": true,
    "message": "Add CI workflow for Python tests"
  }
}